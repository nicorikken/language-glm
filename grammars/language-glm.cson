            name: "storage.type.object.glm"
          "2":
            name: "entity.name.function.object.glm"
          "3":
            name: "constant.id.object.glm"
          "4":
            name: "constant.id.object.glm"
        end: "}"
        patterns: [
          {
            include: "#powerflow-properties-powerflow_object"
