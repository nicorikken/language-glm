      {
        include: "#macro-variable-custom"
      }
      {
        include: "#macro-variable-environment"
      }
      {
        include: "#macro-variable-global"
      }
    ]
  "clock-includes":
    patterns: [
      {
        include: "#clock-timestamps"
      }
      {
        include: "#clock-timezones"
      }
    ]
  modules:
