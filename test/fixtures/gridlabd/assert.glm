// Examples for module assert

// class assert
object assert:0 {

	// header properties
	rank 0;
	clock (invalid);
	flags NONE;
	// assert properties
	status INIT;
	target "";
	part "";
	relation ==;
	value "";
	within "";
	lower "";
	upper "";
}


// class complex_assert
object complex_assert:0 {

	// header properties
	rank 0;
	clock (invalid);
	flags NONE;
	// complex_assert properties
	status 0;
	once ONCE_FALSE;
	operation FULL;
	value +0+0i;
	within +0;
	target "";
}


